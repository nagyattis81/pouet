    "amiga",
    "c64",
    "pc"
  ],
  "author": "Nagy Attila (aha)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nagyattis81/pouet-dump/issues"
  },
  "homepage": "https://github.com/nagyattis81/pouet-dump#readme",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^18.6.2",
    "@types/sqlite3": "^3.1.8",
    "axios-mock-adapter": "^1.21.1",
    "jest": "^28.1.3",
    "mock-fs": "^5.1.4",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^0.27.2",
    "csv-writer": "^1.6.0",
    "rxjs": "^7.5.6",
    "sqlite3": "^5.0.11"
  }
